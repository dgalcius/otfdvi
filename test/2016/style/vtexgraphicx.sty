\def\newpi{\@ifnextchar[{\@newpi}{\@newpi[]}}
\def\@newpi[#1]#2#3{%
  \@ifundefined{pi@#2}{}{\@warning{Label `#2' multiply defined}}%
  \global\@namedef{pi@#2}{#3[#1]}%
  \if.#1.\relax
